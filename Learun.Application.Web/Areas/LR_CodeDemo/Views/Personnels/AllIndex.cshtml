@{
    ViewBag.Title = "个人中心";
    Layout = "~/Views/Shared/_Form.cshtml";
}



<style type="text/css">

    .lr-layout-wrap-item .title {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        padding: 10px 20px 0 20px;
    }

        .lr-layout-wrap-item .title > div {
            position: relative;
            width: 100%;
            border-bottom: 1px solid #ccc;
            line-height: 34px;
            font-size: 12px;
            font-weight: bold;
        }

    .lr-layout-center .lr-layout-wrap {
        padding-top: 0 !important;
    }

    .lr-layout-wrap-item {
        position: relative;
        width: 100%;
        height: 100%;
        display: none;
        padding: 45px 20px 0 20px;
    }

        .lr-layout-wrap-item.active {
            display: block;
        }
</style>
@Html.AppendCssFile("/Areas/LR_CodeDemo/Views/Personnels/Index.css")
<div id="lr_layout" class="lr-layout lr-layout-left-center">
    <div class="lr-layout-left">
        <div class="lr-layout-wrap lr-layout-wrap-notitle" style="padding-top:10px;">
            <div class="lr-layout-body">
                <ul class="lr-left-list" id="lr_left_list">
                    <li class="active" data-value="1">基本信息</li>
                    <li data-value="2">身份证信息</li>
                    <li data-value="3">个人资格证</li>
                    <li data-value="4">毕业证</li>
                    <li data-value="5">培训记录</li>
                    <li data-value="6">从业经历</li>
                    <li data-value="7">合同结算</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="lr-layout-center">
        <div class="lr-layout-wrap">
            <div class="lr-layout-wrap-item active" id="lr_layout_item1">
                <div class="title">
                    <div>基本信息</div>
                </div>
                <div class="content" style="padding-top:20px;">
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">姓名</div>
                        <input id="F_UserName" type="text" class="form-control" readonly />
                    </div>
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">身份证号码</div>
                        <input id="F_IDCardNo" type="text" class="form-control" readonly />
                    </div>
                    @*<div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">性别</div>
                        <div id="F_Gender" isvalid="yes" readonly></div>
                    </div>
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">年龄</div>
                        <input id="F_Age" type="text" class="form-control" readonly />
                    </div>*@
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">存档编号</div>
                        <input id="F_PlaceCode" type="text" class="form-control" readonly />
                    </div>
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">证书编码</div>
                        <input id="F_CertCode" type="text" class="form-control" readonly />
                    </div>
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">供应商</div>
                        <input id="F_ApplicantId" type="text" class="form-control" readonly />
                    </div>
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">到场</div>
                        <div id="F_SceneType" isvalid="yes"  readonly></div>
                    </div>
                    <div class="lr-form-item" data-table="tc_Personnels">
                        <div class="lr-form-item-title">备注</div>
                        <textarea id="F_Description" class="form-control" style="height:100px;" readonly></textarea>
                    </div>
                </div>
            </div>
            <div class="lr-layout-wrap-item" id="lr_layout_item2">
                <div class="title">
                    <div>身份证信息</div>
                </div>
                <div class="content">
                    <iframe class="lr-layout-iframe" id="lr_iframe_item2" style="height:-webkit-fill-available;width:-webkit-fill-available" frameborder="0"></iframe>
                </div>
            </div>

            <div class="lr-layout-wrap-item" id="lr_layout_item3">
                <div class="title">
                    <div>个人资格证</div>
                </div>
                <div class="content">
                    <iframe class="lr-layout-iframe" id="lr_iframe_item3" style="height:-webkit-fill-available;width:-webkit-fill-available" frameborder="0"></iframe>
                </div>
            </div>

            <div class="lr-layout-wrap-item" id="lr_layout_item4">
                <div class="title">
                    <div>毕业证</div>
                </div>
                <div class="content">
                    <iframe class="lr-layout-iframe" id="lr_iframe_item4" style="height:-webkit-fill-available;width:-webkit-fill-available" frameborder="0"></iframe>
                </div>
            </div>
            <div class="lr-layout-wrap-item" id="lr_layout_item5">
                <div class="title">
                    <div>培训记录</div>
                </div>
                <div class="content">
                    <iframe class="lr-layout-iframe" id="lr_iframe_item5" style="height:-webkit-fill-available;width:-webkit-fill-available" frameborder="0"></iframe>
                </div>
            </div>
            <div class="lr-layout-wrap-item" id="lr_layout_item6">
                <div class="title">
                    <div>从业经历</div>
                </div>
                <div class="content">
                    <iframe class="lr-layout-iframe" id="lr_iframe_item6" style="height:-webkit-fill-available;width:-webkit-fill-available" frameborder="0"></iframe>
                </div>
            </div>
            <div class="lr-layout-wrap-item" id="lr_layout_item7">
                <div class="title">
                    <div>合同结算</div>
                </div>
                <div class="content">
                    <iframe class="lr-layout-iframe" id="lr_iframe_item7" style="height:-webkit-fill-available;width:-webkit-fill-available" frameborder="0"></iframe>
                </div>
            </div>

        </div>
    </div>

</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/Personnels/AllIndex.js")

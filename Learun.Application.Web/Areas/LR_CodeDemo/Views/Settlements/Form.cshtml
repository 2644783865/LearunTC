@{
    ViewBag.Title = "合同结算";
    Layout = "~/Views/Shared/_Form.cshtml";
}
    <div class="lr-form-wrap" id="form">

        <div class="col-xs-12 lr-form-item" data-table="tc_Settlements" style="display: none; ">
            <div class="lr-form-item-title">Id</div>
            <input id="F_PersonId" type="text" class="form-control" />
        </div>

        <div class="col-xs-12 lr-form-item" data-table="tc_Settlements" style="display: none; ">
            <div class="lr-form-item-title">Id</div>
            <input id="F_SettlementsId" type="text" class="form-control" />
        </div>


        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">姓名</div>
            <input id="F_UserName" type="text" class="form-control" readonly style="background: rgb(238, 238, 238);" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">身份证号码</div>
            <input id="F_IDCardNo" type="text" class="form-control" readonly style="background: rgb(238, 238, 238);" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">合同起日<font face="宋体">*</font></div>
            <input id="F_ContractStartDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_ContractStartDate').trigger('change'); } })" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">合同止日<font face="宋体">*</font></div>
            <input id="F_ContractEndDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_ContractEndDate').trigger('change'); } })" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">维护代表<font face="宋体">*</font></div>
            <input id="F_Maintain" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">合同状态<font face="宋体">*</font></div>
            <div id="F_ContractStatus" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">手机<font face="宋体">*</font></div>
            <input id="F_Mobile" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>

        <div class="col-xs-12 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">其它方式</div>
            <input id="F_OtherContact" type="text" class="form-control" />
        </div>
        <div class="col-xs-12 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">联系地址</div>
            <input id="F_Address" type="text" class="form-control" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">人才签约代表<font face="宋体">*</font></div>
            <div id="F_ApplicantId" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">收款人<font face="宋体">*</font></div>
            <input id="F_payee" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">开户行<font face="宋体">*</font></div>
            <input id="F_BankName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">银行账号<font face="宋体">*</font></div>
            <input id="F_BankAccount" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">人员薪酬<font face="宋体">*</font></div>
            <input id="F_PersonAmount" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">中介费用<font face="宋体">*</font></div>
            <input id="F_ApplicantAmount" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">签约金额<font face="宋体">*</font></div>
            <input id="F_ContractAmount" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" readonly style="background: rgb(238, 238, 238);" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">付款状态<font face="宋体">*</font></div>
            <div id="F_PayStatus" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">累计支付次数</div>
            <input id="F_PayNumber" type="text" class="form-control" readonly style="background: rgb(238, 238, 238);" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_Settlements">
            <div class="lr-form-item-title">累计支付金额</div>
            <input id="F_PayTotalAmount" type="text" class="form-control" readonly style="background: rgb(238, 238, 238);" />
        </div>
    </div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/Settlements/Form.js")

@{
    ViewBag.Title = "projectmanage";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-tabs" id="lr_form_tabs">
    <ul class="nav nav-tabs">
        <li><a data-value="tab1">项目详情</a></li>
        <li><a data-value="tab2">项目需求</a></li>
    </ul>
</div>
<div class="tab-content lr-tab-content" id="lr_tab_content">
<div class="lr-form-wrap tab-pane" id="tab1">
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">单位名称<font face="宋体">*</font></div>
        <div id="ApplicantId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">注册地址</div>
        <input id="RegisterAddress" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">资质类别</div>
        <div id="ProjectType"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">社保交费<font face="宋体">*</font></div>
        <div id="SocialSecurities" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">资质专业<font face="宋体">*</font></div>
        <input id="Major" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">资质等级<font face="宋体">*</font></div>
        <div id="Rank" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">需求发布日期<font face="宋体">*</font></div>
        <input id="PublishDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#PublishDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">配置完成日期</div>
        <input id="ConfigDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#ConfigDate').trigger('change'); } })"   />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">项目结束日期</div>
        <input id="CompleteDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#CompleteDate').trigger('change'); } })"   />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_Project" >
        <div class="lr-form-item-title">项目状态<font face="宋体">*</font></div>
        <div id="Status" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
</div>
<div class="lr-form-wrap tab-pane" id="tab2">
    <div class="col-xs-12 lr-form-item lr-form-item-grid" >
        <div id="tc_ProjectDetail"></div>
    </div>
</div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/projectmanage/Form.js")

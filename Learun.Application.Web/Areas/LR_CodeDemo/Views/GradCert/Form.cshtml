@{
    ViewBag.Title = "毕业证书";
    Layout = "~/Views/Shared/_Form.cshtml";
}
    <div class="lr-form-wrap" id="form">
        <div class="col-xs-12 lr-form-item" data-table="tc_GradeCert" style="display: none; ">
            <div class="lr-form-item-title">Id</div>
            <input id="F_PersonId" type="text" class="form-control" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">姓名</div>
            <input id="F_UserName" type="text" class="form-control" readonly style="background: rgb(238, 238, 238);" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">身份证号码</div>
            <input id="F_IDCardNo" type="text" class="form-control" readonly style="background: rgb(238, 238, 238);" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">专业<font face="宋体">*</font></div>
            <input id="F_Major" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">毕业时间<font face="宋体">*</font></div>
            <input id="F_GradTime" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_GradTime').trigger('change'); } })" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">学历类型<font face="宋体">*</font></div>
            <div id="F_EducationType" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">学制类型<font face="宋体">*</font></div>
            <div id="F_Term" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">保管方式<font face="宋体">*</font></div>
            <div id="F_OriginalType" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_GradeCert">
            <div class="lr-form-item-title">备注<font face="宋体">*</font></div>
            <input id="F_Description" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
    </div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/GradCert/Form.js")

@{
    ViewBag.Title = "培训记录";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-tabs" id="lr_form_tabs">
    <ul class="nav nav-tabs">
        <li><a data-value="tab1">培训记录</a></li>
        <li><a data-value="tab2">结算信息</a></li>
    </ul>
</div>
<div class="tab-content lr-tab-content" id="lr_tab_content">
    <div class="lr-form-wrap tab-pane" id="tab1">



        <div class="col-xs-12 lr-form-item" data-table="tc_PersonnelTrain" style="display: none; ">
            <div class="lr-form-item-title">Id</div>
            <input id="F_PersonId" type="text" class="form-control" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">姓名</div>
            <input id="F_UserName" type="text" class="form-control" disabled />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">身份证号码</div>
            <input id="F_IDCardNo" type="text" class="form-control" disabled />
        </div>


        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">培训证书类型<font face="宋体">*</font></div>
            <input id="F_CertType" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">培训专业序列<font face="宋体">*</font></div>
            <input id="F_MajorType" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">培训证书专业 <font face="宋体">*</font></div>
            <input id="F_Major" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">培训发证机构<font face="宋体">*</font></div>
            <input id="F_CertOrganization" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">预定发证日期<font face="宋体">*</font></div>
            <input id="F_CertDateBegin" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_CertDateBegin').trigger('change'); } })" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">证书失效日期<font face="宋体">*</font></div>
            <input id="F_CertDateEnd" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_CertDateEnd').trigger('change'); } })" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">资格证形式<font face="宋体">*</font></div>
            <div id="F_CertStyle" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="tc_PersonnelTrain">
            <div class="lr-form-item-title">库存状态<font face="宋体">*</font></div>
            <div id="F_CertStatus" isvalid="yes" checkexpession="NotNull"></div>
        </div>
    </div>
<div class="lr-form-wrap tab-pane" id="tab2">
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">报名培训状态<font face="宋体">*</font></div>
        <div id="F_TrainStatus" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">登记日期<font face="宋体">*</font></div>
        <input id="F_CheckInDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_CheckInDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">报名截止日期<font face="宋体">*</font></div>
        <input id="F_ApplyDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_ApplyDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">预计培训日期<font face="宋体">*</font></div>
        <input id="F_ExpectedTrainDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_ExpectedTrainDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">报考收费标准<font face="宋体">*</font></div>
        <input id="F_FeeStandard" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">报考收费状态<font face="宋体">*</font></div>
        <div id="F_TrainCollectStatus" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">付费代理机构</div>
        <input id="F_TrainOrgName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">代理机构开户行<font face="宋体">*</font></div>
        <input id="F_TrainOrgBankName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">代理机构银行账号<font face="宋体">*</font></div>
        <input id="F_TrainOrgBankAccount" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">付费金额</div>
        <input id="F_TrainPayAmount" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">付费凭证<font face="宋体">*</font></div>
        <div id="F_TrainPayVoucher" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">付费状态<font face="宋体">*</font></div>
        <div id="F_TrainPayStatus" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="tc_PersonnelTrain" >
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Description" class="form-control" style="height:150px;"  ></textarea>
    </div>
</div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/PersonnelTrain/Form.js")
